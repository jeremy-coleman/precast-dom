{
  "name": "@preact/workspace",
  "private": true,
  "zscripts": {
    "start1": "webpack serve",
    "serve": "serve dist/app",
    "start:react": "webpack --config=webpack.react.js",
    "tscheck": "tsc -p tsconfig.json --outDir _checked",
    "dist": "webpack --config=webpack.prod.js --mode production --optimize-minimize",
    "start": "webpack --config=webpack.config.js",
    "prod": "cross-env NODE_ENV=production wp --config ./webpack.config",
    "pretest": "npm run lint",
    "test": "lerna run test",
    "prettier": "prettier --write **/*.{js,ts,tsx,json,css,scss,md,yml}",
    "lint": "eslint .",
    "lintfix": "eslint --fix",
    "less": "lessc src"
  },
  "scripts": {
    "dist": "webpack --config=webpack.production.js --mode production --optimize-minimize",
    "start": "webpack --config=webpack.config.js"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": false,
    "jsxBracketSameLine": false,
    "printWidth": 120,
    "quoteProps": "consistent",
    "semi": false,
    "tabWidth": 2,
    "trailingComma": "none"
  },
  "importSort": {
    ".js, jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "dependencies": {
    "preact": "10.5.12",
    "proptypes": "^1.1.0"
  },
  "devDependencies": {
    "@sucrase/webpack-loader": "2.0.0",
    "@types/node": "^14.14.25",
    "css-loader": "5.0.2",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.0.0",
    "react-hot-loader": "file:./tools/react-hot-loader",
    "style-loader": "^2.0.0",
    "sucrase": "3.17.1",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "4.1.4",
    "webpack": "5.21.2",
    "webpack-dev-middleware": "4.1.0",
    "webpack-hot-middleware": "2.25.0"
  }
}
