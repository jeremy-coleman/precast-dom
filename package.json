{
  "name": "@preact/workspace",
  "private": true,
  "scripts": {
    "dist": "node start --build",
    "start": "node start --serve"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": false,
    "jsxBracketSameLine": false,
    "printWidth": 120,
    "quoteProps": "consistent",
    "semi": false,
    "tabWidth": 2,
    "trailingComma": "none"
  },
  "importSort": {
    ".js, jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "dependencies": {
    "preact": "10.5.12",
    "proptypes": "^1.1.0"
  },
  "devDependencies": {
    "@sucrase/webpack-loader": "2.0.0",
    "@types/node": "^14.14.25",
    "css-loader": "5.0.2",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.0.0",
    "react-hot-loader": "file:./tools/react-hot-loader",
    "style-loader": "^2.0.0",
    "sucrase": "3.17.1",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "4.1.4",
    "webpack": "5.21.2",
    "webpack-dev-middleware": "4.1.0",
    "webpack-hot-middleware": "2.25.0"
  }
}
