{
  "name": "@preact/workspace",
  "private": true,
  "scripts": {
    "serve": "serve dist/app",
    "start:react": "webpack --config=webpack.react.js",
    "tscheck": "tsc -p tsconfig.json --outDir _checked",
    "dist": "webpack --config=webpack.prod.js --mode production --optimize-minimize",
    "start": "webpack --config=webpack.config.js",
    "prod": "cross-env NODE_ENV=production wp --config ./webpack.config",
    "pretest": "npm run lint",
    "test": "lerna run test",
    "prettier": "prettier --write **/*.{js,ts,tsx,json,css,scss,md,yml}",
    "lint": "eslint .",
    "lintfix": "eslint --fix",
    "less": "lessc src"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": false,
    "jsxBracketSameLine": false,
    "printWidth": 100,
    "quoteProps": "consistent",
    "semi": false,
    "tabWidth": 2,
    "trailingComma": "none"
  },
  "importSort": {
    ".js, jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "dependencies": {
    "react-hot-loader": "4.13.0",
    "@hot-loader/react-dom": "17.0.1",
    "emotion": "11.0.0",
    "feather-icons": "^4.28.0",
    "fullscreen": "^1.1.1",
    "highlight.js": "^10.4.1",
    "history": "^5.0.0",
    "lodash-es": "4.17.15",
    "polished": "^4.0.5",
    "preact": "10.5.7",
    "preact-feather": "4.1.0",
    "prismjs": "1.22.0",
    "prop-types": "^15.7.2",
    "react": "17.0.1",
    "react-bootstrap": "^1.4.0",
    "react-dom": "17.0.1",
    "react-router": "^5.2.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "7.12.1",
    "webpack-dev-server": "3.11.0",
    "@babel/plugin-proposal-decorators": "7.12.1",
    "@babel/plugin-transform-react-jsx": "7.12.7",
    "@prefresh/babel-plugin": "0.2.2",
    "@prefresh/webpack": "2.2.1",
    "chalk": "4.1.0",
    "express": "4.17.1",
    "@mixer/parallel-prettier": "2.0.0",
    "webpack-dev-middleware": "4.0.2",
    "webpack-hot-middleware": "2.25.0",
    "eslint": "7.14.0",
    "@babel/core": "7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "7.12.7",
    "@babel/preset-react": "7.12.7",
    "@css-blocks/webpack": "^0.24.0",
    "@mdx-js/loader": "^1.6.22",
    "@types/feather-icons": "^4.7.0",
    "@types/lodash-es": "4.17.3",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "babel-loader": "8.2.2",
    "babel-plugin-module-resolver": "4.0.0",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "5.0.1",
    "file-loader": "6.2.0",
    "fs-jetpack": "4.1.0",
    "gulp": "4.0.2",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "4.5.0",
    "jest": "26.6.3",
    "less": "^3.12.2",
    "less-loader": "^7.1.0",
    "markdown-loader": "^6.0.0",
    "prettier": "2.2.1",
    "remark-highlight.js": "^6.0.0",
    "sass-loader": "10.1.0",
    "style-loader": "2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended": "^3.0.0",
    "ts-jest": "26.4.4",
    "ts-loader": "8.0.11",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "tslint": "6.1.3",
    "typescript": "4.1.2",
    "typescript-tslint-plugin": "^0.5.5",
    "url-loader": "4.1.1",
    "webpack": "5.9.0",
    "webpack-cli": "^4.2.0",
    "webpack-merge": "5.4.0",
    "tapable": "2.1.1",
    "webpack-plugin-serve": "1.2.1",
    "workbox-webpack-plugin": "6.0.2",
    "worker-loader": "3.0.6"
  }
}
